<script>
function cutomToggle(id)
{
	if ( jQuery( "."+id ).is( ":hidden" ) ) {
		jQuery( "."+id ).slideDown( "fast" );
		jQuery( ".toggle"+id ).removeClass( "data" );
		jQuery( ".toggle"+id ).addClass( "data-minus" );
	} else {
		jQuery( "."+id ).slideUp("fast");
		jQuery( ".toggle"+id ).removeClass( "data-minus");
		jQuery( ".toggle"+id ).addClass( "data" );
		
	}
}
