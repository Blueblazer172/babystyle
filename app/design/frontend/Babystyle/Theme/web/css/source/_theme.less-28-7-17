/*** Custom Mixins ***/
.bdrad(@radius){
	-webkit-border-radius:@radius;
	-moz-border-radius:@radius;
	-o-border-radius:@radius;
	-ms-border-radius:@radius;
	border-radius:@radius;
}
.bxs(@sizing:border-box;){
	-webkit-box-sizing:@sizing;
	-moz-box-sizing:@sizing;
	-o-box-sizing:@sizing;
	-ms-box-sizing:@sizing;
	box-sizing:@sizing;
}
.mar (@top: 5px, @right: 5px, @bottom: 5px, @left: 5px) {
	margin: @top @right @bottom @left;
}
.mar2 (@topbt: 5px, @leftrt: 5px) {
	margin:@topbt @leftrt;
}
.mar3 (@top: 5px, @leftrt: 5px, @bt:5px) {
	margin:@top @leftrt @bt;
}
.mar1 (@mar: 5px) {
	margin:@mar;
}

.pad(@top: 5px, @right: 5px, @bottom: 5px, @left: 5px){
	padding: @top @right @bottom @left;
}
.pad2(@topbt: 5px, @leftrt: 5px) {
	padding:@topbt @leftrt;
}
.pad3(@top: 5px, @leftrt: 5px, @bt:5px) {
	padding:@top @leftrt @bt;
}
.pad1(@pad: 5px) {
	padding:@pad;
}
.trnsl(@x:-50%, @y:-50%){
	-webkit-transform:translate(@x, @y);
	-o-transform:translate(@x, @y);
	-moz-transform:translate(@x, @y);
	-ms-transform:translate(@x, @y);
	transform:translate(@x, @y);
}
.fontawsome{
	font-family:fontAwesome;
}

.hoverbefore{
	content: "";
    position: @ab;
    z-index: -1;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: #ffffff;
    -webkit-transform: scaleY(0);
    transform: scaleY(0);
    -webkit-transform-origin: 50%;
    transform-origin: 50%;
    -webkit-transition-property: transform;
    transition-property: transform;
    -webkit-transition-duration: 0.3s;
    transition-duration: 0.3s;
    -webkit-transition-timing-function: ease-out;
    transition-timing-function: ease-out;
	border: 1px solid #ff5c05;
}
.btnhover{
	background: #ff5c05;
    border: 1px solid transparent;
    color: #ffffff;
    font-weight: 400;
    text-transform: uppercase;
	position:@rt;
	-webkit-transform: translateZ(0);
    transform: translateZ(0);
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    -webkit-transition-property: color;
    transition-property: color;
    -webkit-transition-duration: 0.3s;
    transition-duration: 0.3s;
    border-radius: 0;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
}

